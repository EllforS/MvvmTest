<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.ellfors.mvvmtest"
    tools:ignore="MissingVersion">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name=".app.MyApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="UnusedAttribute">

        <activity android:name=".biz.main.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".biz.img.MyImageActivity" />

        <activity android:name=".biz.list.MyArticleActivity" />

        <activity android:name=".biz.list.MyArticleDetActivity" />

        <activity android:name=".biz.time.TimeDownActivity" />

        <activity android:name=".biz.edit.EditActivity" />

        <activity android:name=".biz.viewtype.ViewTypeActivity" />

    </application>

</manifest>